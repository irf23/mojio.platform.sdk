{
  "title": "Mojio Platform SDK CLI V2 (DotNet Core)",

  "description": "Preview of the DotNet Core compatible Mojio SDK CLI",

  "version": "1.0.13-*",
  "buildOptions": {
    "emitEntryPoint": true,
    "define": [ "DOTNETCORE" ]
  },

  "dependencies": {
    "Microsoft.Extensions.Configuration": "1.0.0",
    "Microsoft.Extensions.Configuration.CommandLine": "1.0.0",
    "Microsoft.Extensions.Configuration.Json": "1.0.0",
    "Microsoft.NETCore.App": {
      "type": "platform",
      "version": "1.0.1"
    },
    "Mojio.Platform.SDK.Contracts": "1.0.0-*",
    "Mojio.Platform.SDK.DI": "1.0.0-*",
    "Mojio.Platform.SDK.Live.WebSocket": "1.0.0-*",
    "Mojio.Platform.SDK.SimpleClient": "1.0.0-*",
    "Newtonsoft.Json": "9.0.1"
  },

  "frameworks": {
    "netcoreapp1.0": {
      "imports": "dnxcore50"
    }
  }
}
